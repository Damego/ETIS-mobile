{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "airbnb/hooks",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        "radix": "off",
        "react/function-component-definition": "off",
        "react/prop-types": "off",
        "consistent-return": "off",
        "no-console": "off",
        "global-require": "off",
        "class-methods-use-this": "off",
        "prefer-const": "off",
        "no-param-reassign": "off"
    }
}
